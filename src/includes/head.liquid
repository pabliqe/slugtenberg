<title>{{ config.siteTitle }} &bull; {{ current | unmakeSlug }}</title>
<meta name="description" content="{{ config.siteDescription }}">

<meta charset="utf-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">

<link rel="icon" type="image/png" href="{{ 'favicon.png' | assetLink }}">
<link rel="stylesheet" href="{{ config.stylesUrl }}">

<meta property="og:type" content="website" />
<meta property="og:image" content="{{ 'thumbnail.png' | assetLink }}"/>